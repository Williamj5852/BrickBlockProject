public static void areaCaclulations()
    {
        double wallArea = wallH * wallL;
        double voidArea = voidH * voidL;
        double totalArea = wallArea - voidArea;
        
        JOptionPane.showMessageDialog(null,"Wall Area (inches squared): " + wallArea + 
        "\nVoid Area (inches squared): " + voidArea + "\nTotal Area (inches squared): " 
        + totalArea);
    }
